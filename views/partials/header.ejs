<body>
    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top  header-transparent ">
        <div class="container d-flex align-items-center">

            <div class="logo mr-auto">
                <h2 class="text-light"><a href="/">ASMMS</a></h2>
            </div>

            <nav class="nav-menu d-none d-lg-block">
                <ul>
                    <% if(!currentUser){ %>
                    <li class="active"><a href="#hero">Home</a></li>
                    <li><a href="#features">Features</a></li>
                    <li><a href="#services">Services</a></li>
                    <li><a href="#faq">F.A.Q</a></li>
                    <li><a href="#contact">Contact Us</a></li>
                    <li class="signInModal"><a href="#" id="signInModalTrigger" data-toggle="modal"
                            data-target="#signIn">Sign
                            in</a></li>
                    <% } else if(currentUser.isAdmin){ %>
                    <li class="active"><a href="/admin">Pending Orders</a></li>
                    <li><a href="/orders">Orders</a></li>
                    <li><a href="#">Add funds</a></li>
                    <li><a href="#">Balance:
                            <%= currentUser.balance.toFixed(2) %> $</a></li>
                    <li class="signInModal"><a href="/logout">Logout</a></li>
                    <% }else{ %>
                    <li class="active"><a href="/dashboard">New order</a></li>
                    <li><a href="/orders">Orders</a></li>
                    <li><a href="#" data-toggle="modal" data-target="#addFunds">Add funds</a></li>
                    <li><a href="#" data-toggle="modal" data-target="#addFunds">Balance:
                            <%= currentUser.balance.toFixed(2) %> $</a></li>
                    <li class="signInModal"><a href="/logout">Logout</a></li>
                    <% } %>
                </ul>
            </nav>
            <!-- .nav-menu -->

        </div>

    </header>
    <!-- End Header -->
    <% if(error && error.length > 0){ %>
    <script>
        $(document).ready(function () {
            $("#loginError").modal('show');
        });
    </script>
    <% } %>
    <% if(confirmEmail && confirmEmail.length > 0){ %>
    <script>
        $(document).ready(function () {
            $("#checkEmail").modal('show');
        });
    </script>
    <% } %>
    <% if(success && success.length > 0){ %>
    <script>
        $(document).ready(function () {
            $("#confirmedEmail").modal('show');
        });
    </script>
    <% } %>